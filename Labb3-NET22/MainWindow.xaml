<we:CustomWindow xmlns:we="clr-namespace:Labb3_NET22.DataModels" x:Class="Labb3_NET22.MainWindow"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 mc:Ignorable="d"
                 Title="Quiz app" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Margin="4">

            <Button x:Name="CreateButton" Content="Create quiz" Margin="2" Click="CreateButton_Click" />

            <Button x:Name="EditButton" Content="Edit quiz" Margin="2" Click="EditButton_Click" />

            <Button x:Name="PlayButton" Content="Play" Margin="2" Click="PlayButton_Click" />
        </StackPanel>

    </Grid>
</we:CustomWindow>